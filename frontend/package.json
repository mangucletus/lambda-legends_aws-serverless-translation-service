{
  "name": "aws-translate-frontend",
  "version": "2.0.0",
  "description": "React frontend for AWS Translate application with Amplify v6 and email-only authentication",
  "private": true,
  "dependencies": {
    "@aws-amplify/ui-react": "^6.1.8",
    "aws-amplify": "^6.0.25",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "eslint": "^8.57.0",
    "prettier": "^3.1.0",
    "web-vitals": "^3.5.0"
  },
  "scripts": {
    "start": "GENERATE_SOURCEMAP=false react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "build-prod": "GENERATE_SOURCEMAP=false NODE_ENV=production react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.{js,jsx} --max-warnings=0",
    "lint:fix": "eslint src/**/*.{js,jsx} --fix",
    "format": "prettier --write src/**/*.{js,jsx,css,json}",
    "format:check": "prettier --check src/**/*.{js,jsx,css,json}",
    "clean": "rm -rf build node_modules package-lock.json && npm install",
    "clean:cache": "rm -rf node_modules/.cache && npm start",
    "deploy": "npm run build-prod && echo 'Build completed successfully'",
    "analyze": "npm run build && npx source-map-explorer 'build/static/js/*.js'",
    "serve": "npx serve -s build -l 3000"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "warn",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie <= 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "aws",
    "translate",
    "react",
    "translation",
    "serverless",
    "amplify",
    "cognito",
    "lambda",
    "s3"
  ],
  "author": "Team Lambda Legends",
  "license": "MIT",
  "homepage": ".",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "overrides": {
    "react-scripts": {
      "typescript": "^4.9.5"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/reportWebVitals.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  }
}